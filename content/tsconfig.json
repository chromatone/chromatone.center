{
	"include": [
		"**/*.*",
		"*.md",
		"*.vue"
	],
	"typedocOptions": {
		"entryPoints": [
			"../use/",
			"./"
		],
		"out": "support/code/docs/",
		"githubPages": false,
		"entryPointStrategy": "expand",
		"name": "Use-chromatone documentation",
		"defaultCategory": "Chomatone",
		"exclude": [
			"**/aubio.js"
		],
		"readme": "use/README.md"
	},
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"#/*": [
				"../*"
			]
		},
		"jsx": "preserve",
		"esModuleInterop": true,
		"target": "esnext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"allowJs": true,
		"isolatedModules": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"declarationMap": true,
		"declarationDir": "lib/types",
		"skipLibCheck": true,
		"module": "esnext",
		"allowSyntheticDefaultImports": true,
		"outDir": "lib/types"
	},
	"vueCompilerOptions": {
		"plugins": [
			"@vue/language-plugin-pug"
		]
	}
}