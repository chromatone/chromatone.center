{
	"allowJs": true,
	"vueCompilerOptions": {
		"plugins": ["@volar/vue-language-plugin-pug"]
	},
	"include": [".vitepress/use/**/*"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"#/*": ["./.vitepress/*"]
		},
		"target": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"allowJs": true,
		"isolatedModules": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"declarationMap": true,
		"declarationDir": "lib/types",
		"skipLibCheck": true,
		"module": "ESNext",
		"allowSyntheticDefaultImports": true,
		"outDir": "lib/types"
	}
}
