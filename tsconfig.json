{
	"allowJs": true,
	"vueCompilerOptions": {
		"plugins": [
			"@volar/vue-language-plugin-pug"
		]
	},
	"include": [
		".vitepress/use/*"
	],
	"compilerOptions": {
		"baseUrl": ".",
		"target": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"allowJs": true,
		"isolatedModules": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"declarationMap": true,
		"declarationDir": "lib/types",
		"skipLibCheck": true,
		"module": "ESNext",
		"allowSyntheticDefaultImports": true,
		"outDir": "lib/types"
	},
	"typedocOptions": {
		"entryPoints": [
			".vitepress/use/"
		],
		"out": "support/code/docs/",
		"githubPages": false,
		"entryPointStrategy": "expand",
		"name": "Use-chromatone documentation",
		"defaultCategory": "Chomatone",
		"exclude": [
			"**/index.ts",
			"**/aubio.js"
		],
		"readme": ".vitepress/use/README.md"
	}
}